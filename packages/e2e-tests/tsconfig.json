{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"lib": ["ES2022", "DOM"],
		"moduleResolution": "bundler",
		"types": ["node", "@playwright/test"],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"jsx": "preserve"
	},
	"include": ["e2e/**/*", "playwright.config.ts"],
	"exclude": ["node_modules", "../extension"]
}
